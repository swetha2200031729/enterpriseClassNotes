<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  <servlet-name>CreatTable</servlet-name>
  <servlet-class>com.klef.CreatTable</servlet-class>
  </servlet>
  	
  <servlet>
  	<servlet-name>InsertForm</servlet-name>
  	<servlet-class>com.klef.InsertForm</servlet-class>
  </servlet>
  
  <servlet-mapping>
  	<servlet-name>CreatTable</servlet-name>
  	<url-pattern>/creat</url-pattern>
  </servlet-mapping>
  
 <servlet-mapping>
  	<servlet-name>InsertForm</servlet-name>
  	<url-pattern>/insertform</url-pattern>
  </servlet-mapping>
  
  <context-param>
  	<param-name>driver</param-name>
  	<param-value>oracle.jdbc.driver.OracleDriver</param-value>
  </context-param>
    <context-param>
  	<param-name>url</param-name>
  	<param-value>jdbc:oracle:thin:@localhost:1521:xe</param-value>
  </context-param>
  
</web-app>
