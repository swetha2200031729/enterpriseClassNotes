<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>ServletInterfaceDemo</servlet-name>
  	<servlet-class>com.klef.ServletInterfaceDemo</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>GenericDemo</servlet-name>
  	<servlet-class>com.klef.GenericDemo</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ServletInterfaceDemo1</servlet-name>
  	<servlet-class>com.klef.ServletInterfaceDemo1</servlet-class>
  </servlet>
  
  <servlet-mapping>
  	<servlet-name>ServletInterfaceDemo</servlet-name>
  	<url-pattern>/sid</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>GenericDemo</servlet-name>
  	<url-pattern>/gid</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ServletInterfaceDemo1</servlet-name>
  	<url-pattern>/sid1</url-pattern>
  </servlet-mapping>
  
  <servlet>
        <servlet-name>HttpClassDemo</servlet-name>
        <servlet-class>com.klef.HttpClassDemo</servlet-class>
    </servlet>
  <servlet>
  	<servlet-name>Calculator_app</servlet-name>
  	<display-name>Calculator_app</display-name>
  	<description></description>
  	<servlet-class>calculator.Calculator_app</servlet-class>
  </servlet>
    <servlet-mapping>
        <servlet-name>HttpClassDemo</servlet-name>
        <url-pattern>/hid</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
    	<servlet-name>Calculator_app</servlet-name>
    	<url-pattern>/calc</url-pattern>
    </servlet-mapping>
  
</web-app>
